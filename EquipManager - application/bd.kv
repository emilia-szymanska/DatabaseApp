	confbtn: confbtn 
	bckbtn: bckbtn
	eqpcont: eqpcont

	id: parent

	GridLayout:
		cols: 2
		rows: 2
		Label:
			text: "Oddajesz sprzet:"
			size_hint: 0.1, 0.5
		Label:
			id: eqpcont	
			size_hint: 0.9, 0.5
			text: ""
		Button:
			id: confbtn
			size_hint: 0.5, 0.5
			text: "Zatwierdz"
			on_press: parent.SubmitReturning()
		Button:
			id: bckbtn
			size_hint: 0.5, 0.5
			text: "Anuluj"
			on_press: parent.GetBack()
		

</ConfirmReturningEqpScreen>

<ChooseTypeOrderScreen>
	name: "wybor typu zamowienia"
	usordbtn: usordbtn
	unusordbtn: unusordbtn
	bckbtn: bckbtn

	id: parent

	GridLayout:
		cols: 1
		rows: 3
					
		Button:
			id: usordbtn
			text: "Zloz zamowienie zuzywalne"
			on_press: parent.GetToUsOrd()
		
		Button:
			id: unusordbtn
			text: "Zloz zamowienie niezuzywalne"
			on_press: parent.GetToUnUsOrd()

		Button:
			id: bckbtn
			text: "Powrot"
			on_press: parent.GetBack()

</ChooseTypeOrderScreen>

<UsEqpList>
	viewclass:'SelectableLabel'
	SelectableRecycleBoxLayout:
		default_size: None, dp(56)
		default_size_hint: 1, None
		size_hint_y: None
		height: self.minimum_height
		orientation: 'vertical'
		multiselect: False
		touch_multiselect: False
</UsEqpList>

<UnUsEqpList>
	viewclass:'SelectableLabel'
	SelectableRecycleBoxLayout:
		default_size: None, dp(56)
		default_size_hint: 1, None
		size_hint_y: None
		height: self.minimum_height
		orientation: 'vertical'
		multiselect: False
		touch_multiselect: False
</UnUsEqpList>

<ChosenList>
	viewclass:'SelectableLabel'
	SelectableRecycleBoxLayout:
		default_size: None, dp(56)
		default_size_hint: 1, None
		size_hint_y: None
		height: self.minimum_height
		orientation: 'vertical'
		multiselect: False
		touch_multiselect: False
</ChosenList>

<MakeUsOrderScreen>
	name: "zloz zamowienie zuzywalne"
	kindsel: kindsel
	srch: srch
	srchbtn: srchbtn
	eqplst: eqplst
	chosenlst: chosenlst
	num: num
	bckbtn: bckbtn
	confbtn: confbtn
	delbtn: delbtn
	addbtn: addbtn
	
	id: parent

	FloatLayout:
		Label:
			text: "Wybierz sprzet z ponizszej listy"
			size_hint: 0.25, 0.1
			pos_hint: {"x": 0, "y": 0.9}
					
		Spinner:
			id: kindsel
			text: "Wybierz rodzaj"	
			values: parent.kind
			on_text: parent.UpdateContentKind(kindsel.text)
			size_hint: 0.25, 0.1
			pos_hint: {"x": 0.25, "y": 0.9}
					
		TextInput:
			id: srch
			multiline: False
			text: ""
			size_hint: 0.25, 0.1
			pos_hint: {"x": 0, "y": 0.8}
		Button:
			id: srchbtn
			text: "Szukaj"
			size_hint: 0.25, 0.1
			on_press: parent.Search(kindsel.text, srch.text)
			pos_hint: {"x": 0.25, "y": 0.8}
					
		UsEqpList:
			id: eqplst
			size_hint: 0.5, 0.6
			pos_hint: {"x": 0, "y": 0.2}
				
		Label:
			text: "Liczba: "
			size_hint: 0.1, 0.1
			pos_hint: {"x": 0, "y": 0.1}
		TextInput:
			id: num
			size_hint: 0.4, 0.1
			pos_hint: {"x": 0.1, "y": 0.1}
		Button:
			id: addbtn
			text: "Dodaj element do zamowienia"
			size_hint: 0.5, 0.1	
			pos_hint: {"x": 0, "y": 0}
			on_press: parent.AddtoCart()
				
		Label:
			pos_hint: {"x": 0.5, "y": 0.9}
			size_hint: 0.5, 0.1
			text: "Wybrane sprzety"
			
		ChosenList:
			id: chosenlst
			size_hint: 0.5, 0.7
			pos_hint: {"x": 0.5, "y": 0.2}

		Button:
			text: "Usun z wybranych"
			id: delbtn
			size_hint: 0.25, 0.1
			pos_hint: {"x": 0.5, "y": 0.1}
			on_press: parent.PopFromCart()
			
		Button:
			text: "Zloz zamowienie"
			id: confbtn
			size_hint: 0.25, 0.1
			pos_hint: {"x": 0.75, "y": 0.1}
			on_press: parent.SubmitUsOrder()
			
		Button:
			text: "Powrot"
			id: bckbtn
			size_hint: 0.5, 0.1
			pos_hint: {"x": 0.5, "y": 0}
			on_press: parent.GetBack()
	
</MakeUsOrderScreen>

